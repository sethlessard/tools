{
	"name": "t00ls",
	"displayName": "t00ls",
	"description": "t00ls",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:t00ls.newProject"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "t00ls.newProject",
				"title": "t00ls: New Project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"chalk": "4.0.0",
		"clui": "0.3.6",
		"create-react-app": "3.4.1",
		"create-react-library": "3.0.9",
		"fs-extra": "9.0.0",
		"minimist": "1.2.5",
		"rimraf": "3.0.2"
	},
	"devDependencies": {
		"@types/fs-extra": "9.0.2",
		"@types/glob": "7.1.3",
		"@types/mocha": "8.0.0",
		"@types/node": "12.11.7",
		"@types/vscode": "1.50.0",
		"@typescript-eslint/eslint-plugin": "4.1.1",
		"@typescript-eslint/parser": "4.1.1",
		"eslint": "7.9.0",
		"glob": "7.1.6",
		"mocha": "8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "1.4.0"
	}
}
